{% extends 'base.html.twig' %}
{% block title %}
    The best cars in the internet!
{% endblock %}
{% block body %}
<h1>Our offer</h1>
    <table class="table">
        <tr>
            <th>make</th>
            <th>name</th>
        </tr>
        {% for car in cars %}
            <tr>
            <td>
                {{ car.make }}
            </td>
            <td>
                {{ car.name }}
            </td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}